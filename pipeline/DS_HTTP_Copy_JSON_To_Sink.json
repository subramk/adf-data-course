{
	"name": "DS_HTTP_Copy_JSON_To_Sink",
	"properties": {
		"description": "Copy to a Sink which is a JSON",
		"activities": [
			{
				"name": "CopyJsonToSink",
				"description": "Copy JSON  to Sink",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".json"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "PricingDataSource_JSON",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "SinkForJsonDataSource",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": [],
		"lastPublishTime": "2024-05-25T18:25:01Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}